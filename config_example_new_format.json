{
  "说明": "新的参数配置格式示例 - 方案1：默认值优先，特殊标记使用生成器",
  "特殊标记说明": {
    "<TRADE_DATE>": "使用交易日期生成器，适用于日线数据",
    "<MONTHLY_DATE>": "使用月度日期生成器，适用于月度数据"
  },
  "data_sources": {
    "tushare": {
      "data_definitions": {
        "stock_basic": {
          "description": "基础信息数据 - 无参数",
          "storage_type": "SNAPSHOT",
          "required_params": {}
        },
        "trade_cal": {
          "description": "交易日历 - 固定默认值",
          "storage_type": "SNAPSHOT",
          "required_params": {
            "start_date": "20000101",
            "end_date": "20990101"
          }
        },
        "daily": {
          "description": "日线行情 - 使用日期生成器",
          "storage_type": "DAILY",
          "required_params": {
            "trade_date": "<TRADE_DATE>"
          }
        },
        "daily_with_limit": {
          "description": "日线行情 - 混合参数（生成器+默认值）",
          "storage_type": "DAILY",
          "required_params": {
            "trade_date": "<TRADE_DATE>",
            "limit": 5000,
            "offset": 0
          }
        },
        "stock_daily_with_code": {
          "description": "特定股票日线 - 混合参数（生成器+股票代码+限制）",
          "storage_type": "DAILY",
          "required_params": {
            "trade_date": "<TRADE_DATE>",
            "ts_code": "000001.SZ",
            "limit": 1000,
            "adj": "qfq"
          }
        },
        "monthly_income": {
          "description": "月度利润表 - 使用月度日期生成器",
          "storage_type": "MONTHLY",
          "required_params": {
            "monthly_date": "<MONTHLY_DATE>"
          }
        },
        "monthly_income_with_params": {
          "description": "月度利润表 - 混合参数（生成器+默认值）",
          "storage_type": "MONTHLY",
          "required_params": {
            "monthly_date": "<MONTHLY_DATE>",
            "limit": 5000,
            "offset": 0,
            "comp_type": "1"
          }
        },
        "index_daily": {
          "description": "指数日线 - 混合参数示例",
          "storage_type": "DAILY",
          "required_params": {
            "trade_date": "<TRADE_DATE>",
            "ts_code": "000001.SH",
            "start_date": "20100101",
            "end_date": "20991231"
          }
        },
        "fund_nav": {
          "description": "基金净值 - 复杂混合参数",
          "storage_type": "DAILY",
          "required_params": {
            "nav_date": "<TRADE_DATE>",
            "market": "E",
            "limit": 8000,
            "offset": 0,
            "status": "L"
          }
        }
      }
    }
  },
  "向后兼容性": {
    "说明": "系统仍支持原有的列表格式",
    "示例": {
      "old_format": ["trade_date"],
      "new_format": {"trade_date": "<TRADE_DATE>"}
    }
  },
  "优势": [
    "支持默认值，减少重复配置",
    "特殊标记清晰表达意图",
    "支持复杂的混合参数场景",
    "保持向后兼容性",
    "配置更加灵活和强大"
  ]
}