{
  
  "field_validations": {
    "monthly_date": {
      "type": "string",
      "pattern": "^\\d{6}$",
      "description": "年月格式，6位数字，如202412",
      "examples": ["202412", "202501", "202502"],
      "error_message": "monthly_date必须是6位数字格式的年月，如202412"
    },
    "trade_date": {
      "type": "string",
      "pattern": "^\\d{8}$",
      "description": "日期格式，8位数字，如20241231",
      "examples": ["20241231", "20250101", "20250907"],
      "error_message": "trade_date必须是8位数字格式的日期，如20241231"
    },
    "start_date": {
      "type": "string",
      "pattern": "^\\d{8}$",
      "description": "开始日期格式，8位数字，如20241231",
      "examples": ["20100101", "20200101", "20240101"],
      "error_message": "start_date必须是8位数字格式的日期，如20241231"
    },
    "end_date": {
      "type": "string",
      "pattern": "^\\d{8}$",
      "description": "结束日期格式，8位数字，如20241231",
      "examples": ["20241231", "20251231", "20990101"],
      "error_message": "end_date必须是8位数字格式的日期，如20241231"
    },
    "quarterly_date": {
      "type": "string",
      "pattern": "^\\d{8}$",
      "description": "季报日期格式，8位数字，季末日期如20241231",
      "examples": ["20240331", "20240630", "20240930", "20241231"],
      "error_message": "quarterly_date必须是8位数字格式的季末日期，如20241231"
    },
    "archive_types": {
      "type": "string",
      "enum": ["SNAPSHOT", "DAILY", "MONTHLY", "QUARTERLY"],
      "description": "归档类型，限制为四种预定义类型之一",
      "examples": ["SNAPSHOT", "DAILY", "MONTHLY", "QUARTERLY"],
      "error_message": "archive_types必须是以下值之一：SNAPSHOT、DAILY、MONTHLY、QUARTERLY",
      "case_sensitive": true,
      "notes": [
        "SNAPSHOT: 快照数据，如基础信息、配置数据",
        "DAILY: 日度数据，如日线行情、每日指标",
        "MONTHLY: 月度数据，如财务报表数据",
        "QUARTERLY: 季报数据，如季度财务报表"
      ]
    },
    "storage_type": {
      "type": "string",
      "enum": ["SNAPSHOT", "DAILY", "MONTHLY", "QUARTERLY"],
      "description": "存储类型，与archive_types相同的枚举值",
      "examples": ["SNAPSHOT", "DAILY", "MONTHLY", "QUARTERLY"],
      "error_message": "storage_type必须是以下值之一：SNAPSHOT、DAILY、MONTHLY、QUARTERLY",
      "case_sensitive": true
    },
    "data_source": {
      "type": "string",
      "enum": ["tushare", "akshare"],
      "description": "数据源名称，当前支持的数据源",
      "examples": ["tushare", "akshare"],
      "error_message": "data_source必须是支持的数据源之一",
      "extensible": true,
      "notes": ["可扩展支持更多数据源"]
    },
    "lookback_periods": {
      "type": "integer",
      "minimum": 1,
      "maximum": 10000,
      "description": "回溯周期数，表示需要获取多少个周期的历史数据",
      "examples": [1, 30, 365, 1095, 3650],
      "error_message": "lookback_periods必须是1到10000之间的整数"
    },
    "required_params": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "必需参数列表",
      "examples": [["trade_date"], ["monthly_date"], []],
      "error_message": "required_params必须是字符串数组"
    }
  }
}