{
  "path_generator": {
    "name": "path_generator",
    "generator_type": "generic",
    "base_path": "/Users/daishun/个人文档/caiyuangungun/data/raw",
    "paths": {
      "landing_subpath": "landing",
      "archive_subpath": "archive"
    },
    "archive_types": {
      "SNAPSHOT": {
        "value": "snapshot",
        "description": "快照归档",
        "path_pattern": "{source_name}/{data_type}",
        "validation_rules": {
          "required_fields": ["source_name", "data_type", "archive_type"]
        },
        "enabled": true
      },
      "DAILY": {
        "value": "daily",
        "description": "交易日归档",
        "path_pattern": "{source_name}/{data_type}/{year_month}/{day}",
        "validation_rules": {
          "required_fields": ["source_name", "data_type", "archive_type", "daily_date"]
        },
        "enabled": true
      },
      "MONTHLY": {
        "value": "monthly",
        "description": "月度归档",
        "path_pattern": "{source_name}/{data_type}/{year_month}",
        "validation_rules": {
          "required_fields": ["source_name", "data_type", "archive_type", "monthly_date"]
        },
        "enabled": true
      },
      "QUARTERLY": {
        "value": "quarterly",
        "description": "季报归档",
        "path_pattern": "{source_name}/{data_type}/{year_quarter}",
        "validation_rules": {
          "required_fields": ["source_name", "data_type", "archive_type", "quarterly_date"]
        },
        "enabled": true
      }
    },
    "file_config": {
      "filename_template": "data.{file_type}",
      "supported_formats": ["parquet", "json"],
      "default_format": "parquet"
    },

    "description": "通用数据路径生成器配置 - 统一路径管理",
    "version": "2.0",
    "enabled": true
  }
}